{
  "name": "vcloud-sdk-nodejs",
  "version": "1.3.0",
  "description": "vcloud nodejs sdk",
  "author": "chenjian.bzh@bytedance.com",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TTvcloud/vcloud-sdk-nodejs"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "aws-sdk": "^2.297.0",
    "deepmerge": "^2.1.1",
    "node-fetch": "^2.6.0",
    "query-string": "^5.1.1",
    "url": "^0.11.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.5",
    "@types/node-fetch": "^2.3.4",
    "@types/query-string": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.28",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.17.1",
    "replace": "^1.1.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "build:watch": "rm -rf lib && tsc -w",
    "lint": "eslint . --ext ts,js --fix",
    "lint-check": "eslint . --ext ts,js",
    "test": "jest --config ./jest.config.json --coverage --env=node",
    "commit": "commit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "patch": "./scripts/release.sh -r patch",
    "minor": "./scripts/release.sh -r minor",
    "major": "./scripts/release.sh -r major"
  },
  "keywords": [
    "openapi",
    "node",
    "sdk"
  ],
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "./jest.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier",
      "eslint --fix",
      "git add"
    ]
  }
}
